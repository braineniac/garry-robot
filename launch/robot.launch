<launch>

    <group ns="arduino">

        <node name="serial_python" pkg="rosserial_python" type="serial_node.py" output="screen">

            <param name="port" value="/dev/ttyACM0" />

        </node>

    </group>

    <include file="$(find garry-description)/launch/state.launch"/>

    <include file="$(find garry-robot)/launch/camera.launch"/>

    <node name="eyes_node" pkg="garry-robot" type="eyes_node" output="screen"/>
    
</launch>
