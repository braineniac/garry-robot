    <node name="webcam_cv" pkg="cv_camera" type="cv_camera_node" output="screen">

        <param name="device_id" value="0"/>
        <param name="camera_info_url" value="file://$(find garry-robot)/config/webcam.yaml"/>
        <param name="rate" value="30"/>

        <remap from="/webcam_cv/camera_info" to="/webcam/camera_info"/>
        <remap from="/webcam_cv/image_raw" to="/webcam/image_raw"/>
        <remap from="/webcam/image_raw/compressed" to="/webcam/compressed"/>
        <remap from="/webcam/compressed/parameter_descriptions" to="/webcam/parameter_descriptions"/>
        <remap from="/webcam/compressed/parameter_updates" to="/webcam/parameter_updates"/>

    </node>

